<div class="card card-title text-dark bg-primary mb-3">
    <h5>Costo de ultima venta</h5>
    <section class="card-body">
        <% if (cmv != "") { %>
         
        
        <p class="card-text">Esencia: <%= cmv[0].totalEsencias %>
        </p>
        <p>Base: <%= cmv[0].Vg + cmv[0].Pg %>
        </p>
        <p>Nico <%= cmv[0].nico %>
        </p>
        <p>Frasco <%= cmv[0].frasco %>
        </p>
        <p>Cliente: <%= cmv[0].customer %>
        </p>
    </section>
    <h6>Costo total : <%=(cmv[0].totalEsencias+cmv[0].Vg + cmv[0].Pg+cmv[0].nico+cmv[0].frasco) *cmv[0].quantity%>
    </h6>
    <h6>Precio Venta: <%= cmv[0].precioVenta %>
    </h6>
    <h5>Ganacia: <%=cmv[0].precioVenta - ((cmv[0].totalEsencias+cmv[0].Vg + cmv[0].Pg+cmv[0].nico+cmv[0].frasco)*cmv[0].quantity) %>
    </h5>
    <% } %>
</div>
